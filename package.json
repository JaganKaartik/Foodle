{
  "name": "express-foodle-api",
  "version": "1.0.0",
  "description": "Express API for Foodle",
  "main": "index.js",
  "repository": "https://github.com/JaganKaartik/Express-Foodle-API",
  "author": "JaganKaartik <kaartikjagan@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "dev": "yarn build && nodemon dist/index",
    "start": "node dist/index",
    "lint": "eslint '*/**/*.ts' --fix",
    "p": "prettier '*/**/*.ts' --color",
    "seed": "node dist/config/seed",
    "docker-local": "docker-compose --env-file .env.test up"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn p",
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/express-validator": "^3.0.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.14.6",
    "@types/passport": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "mongoose": "^5.10.11",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0",
    "passport-twitter": "^1.0.4"
  }
}
